{
  "extends": ["tslint:latest"],
  "rules": {
    "quotemark": [true, "single"],
    "member-access": false,
    "interface-name": false,
    "ordered-imports": [true, "alphabetize", "check-groups"],
    "switch-default": false,
    "variable-name": [true, "ban-keywords", "check-format", "allow-leading-underscore"]
  }
}
